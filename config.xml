<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets"  xmlns:gap="http://phonegap.com/ns/1.0" id="com.iosbuild.patient" version="1.0.0">
 
    <name>For iOS build</name>
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
	<preference name="SplashScreen" value="splash" />
	<preference name="SplashScreenDelay" value="10000"/>
	
    <gap:platform name="ios" />
    <gap:platform name="android" />
    <description>
      iOS Build Health App
    </description> 
    <author href="http://www.arth.tech" email="support@arth.tech">
    </author>
    <icon src="icon.png" />
	
	
	<platform name="android">
    <splash src="splash/splash.png" />
    <splash src="splash/drawable-port-ldpi-screen.png" platform="android" qualifier="ldpi" />
    <splash src="splash/drawable-port-mdpi-screen.png" platform="android" qualifier="mdpi" />
    <splash src="splash/drawable-port-hdpi-screen.png" platform="android" qualifier="hdpi" />
    <splash src="splash/drawable-port-xhdpi-screen.png" platform="android" qualifier="xhdpi" />
<!--    <splash src="splash/fr-xhdpi.png" platform="android" qualifier="fr-xhdpi" /> -->
    <splash src="splash/drawable-port-xxhdpi-screen.png" platform="android" qualifier="port-xxhdpi" />
  </platform>
    
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="android-build-tool" value="gradle" />
    
    <plugin name="Cordova-Plugin-OpenTok-JBS" version="1.0.0" source="npm" />
    
    <!-- <gap:plugin name="de.appplant.cordova.plugin.printer" />-->
    <gap:plugin name="de.appplant.cordova.plugin.printer" source="npm"/>

    <!-- <plugin name="org.crosswalk.engine" spec="1.3.0" source="pgb" /> -->
    <!-- <gap:plugin name="cordova-plugin-crosswalk-jb" source="npm" /> -->

    <gap:plugin name="cordova-plugin-crosswalk-webview-pgb-adapt" source="npm" />

    <gap:plugin name="org.apache.cordova.splashscreen" source="npm" />
    <gap:plugin name="org.apache.cordova.inappbrowser" source="npm" />
    <gap:plugin name="org.apache.cordova.file" source="npm" />
    <gap:plugin name="org.apache.cordova.file-transfer" source="npm" />
    <gap:plugin name="org.apache.cordova.camera" source="npm" />
    <gap:plugin name="org.apache.cordova.device" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-x-toast" source="npm" />
    <gap:plugin name="org.apache.cordova.statusbar" source="npm" />

    <plugin name="com.onesignal.plugins.onesignal" spec="1.9.0" source="pgb" />
<!--     <gap:plugin name="onesignal-cordova-plugin-pgb-compat" source="npm" />
    <gap:plugin name="cordova-plugin-android-support-v4" source="npm" />
    <gap:plugin name="pgb-google-play-services" source="npm" />
    <gap:plugin name="react-native-onesignal " source="npm" /> -->
    
    <gap:plugin name="cordova-plugin-network-information" source="npm" />
    <!-- <gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay"  version="1.2.0" /> -->
    <gap:plugin name="cordova-plugin-statusbar" source="npm" />
    <!-- <gap:plugin name="com.pushwoosh.plugins.pushwoosh" version="3.4.2" />-->
    
    <access origin="*"/> <!-- allow all pages, use for development -->
    <allow-navigation href="*" />
    
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <!--    <plugin name="com.tokbox.cordova.opentok" spec="1.0.3" source="pgb" />-->

    
    

</widget>